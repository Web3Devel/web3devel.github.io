  x3do:hasParent :Transform_2_3_2_2 ;
  x3do:hasAppearance :BULB_CASING-USE-1 ;
  x3do:hasGeometry :Cylinder_2_3_2_2_1_2 .
:BULB_CASING-USE-1 a owl:NamedIndividual, x3do:Appearance ;
  x3do:hasParent :Shape_2_3_2_2_1 ;
