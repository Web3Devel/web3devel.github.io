NavigationInfo { type [ "EXAMINE" "ANY" ] } ###  Default X3D NavigationInfo

Viewpoint {
  description "View shape"
  position 0 0 6
}
# 45 degrees equals 0.785 radians, 0 1 0 indicates Y axis
Transform {
  rotation 0 1 0 0.785
  children [
      Transform {
        translation 0 0.8 0
        children [
            Shape {
              geometry Cone {
                bottomRadius 0.3
                height 0.6
              }
              appearance DEF GREEN_APPEARANCE Appearance {
                material Material {
                  diffuseColor 0.1 0.7 0.2
                }
              }
            }
        ]
      }
      Shape {
        geometry Box {
          size 0.25 1 0.25
        }
        appearance USE GREEN_APPEARANCE
      }
  ]
}
