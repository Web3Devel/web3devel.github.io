  x3do:hasParent :Group_2_3_2 ;
  x3do:DEF 'SOCKET_TOUCHSENSOR' ;
  x3do:description 'touch to change light intensity' .
:Transform_2_3_3 a owl:NamedIndividual, x3do:Transform ;
  x3do:hasParent :Transform_2_3 ;
  x3do:hasChildren :Shape_2_3_3_1, :LIGHTSOURCE ;
  x3do:translation ( 0 -2.95 0 ) .
