  x3do:hasParent :Scene ;
  x3do:fromField 'isOver' ;
  x3do:fromNode 'SOCKET_TOUCHSENSOR' ;
  x3do:toField 'enabled' ;
  x3do:toNode 'LIGHT_TIMESENSOR' .
