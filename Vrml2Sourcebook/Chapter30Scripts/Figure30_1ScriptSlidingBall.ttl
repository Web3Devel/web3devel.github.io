:MoverUsingContainedScript a owl:NamedIndividual, x3do:Script ;
  x3do:hasParent :Group_2_4 ;
  x3do:hasField :field_2_4_6_1, :field_2_4_6_2 ;
  x3do:DEF 'MoverUsingContainedScript' ;
  x3do:sourceCode """
ecmascript:

// Move a shape in a straight path
function set_fraction( fraction, eventTime ) {
	value_changed[0] = fraction;    // X component
	value_changed[1] = 0.0;         // Y component
	value_changed[2] = 0.0;         // Z component
}
""" .
:field_2_4_6_1 a owl:NamedIndividual, x3do:field ;
  x3do:hasParent :MoverUsingContainedScript ;
  x3do:accessType 'inputOnly' ;
  x3do:appinfo 'receive fraction from clock' ;
  x3do:name 'set_fraction' ;
  x3do:type 'SFFloat' .
:field_2_4_6_2 a owl:NamedIndividual, x3do:field ;
  x3do:hasParent :MoverUsingContainedScript ;
  x3do:accessType 'outputOnly' ;
  x3do:appinfo 'produce output position to move the ball' ;
  x3do:name 'value_changed' ;
  x3do:type 'SFVec3f' .
:ROUTE_2_5 a owl:NamedIndividual, x3do:ROUTE ;
  x3do:hasParent :Scene ;
  x3do:fromField 'fraction_changed' ;
  x3do:fromNode 'Clock' ;
  x3do:toField 'set_fraction' ;
  x3do:toNode 'MoverUsingContainedScript' .
:ROUTE_2_6 a owl:NamedIndividual, x3do:ROUTE ;
  x3do:hasParent :Scene ;
  x3do:fromField 'value_changed' ;
  x3do:fromNode 'MoverUsingContainedScript' ;
  x3do:toField 'set_translation' ;
  x3do:toNode 'BallTransform' .
