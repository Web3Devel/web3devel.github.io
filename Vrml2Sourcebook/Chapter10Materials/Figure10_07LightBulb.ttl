  x3do:hasParent :BULB_CASING ;
  x3do:DEF 'SOCKET_MATERIAL' ;
  x3do:diffuseColor ( 0.4 0.4 0.16 ) ;
  x3do:emissiveColor ( 0.4 0.4 0.16 ) .
:Sphere_2_3_2_1_1_2 a owl:NamedIndividual, x3do:Sphere ;
  x3do:hasParent :Shape_2_3_2_1_1 ;
