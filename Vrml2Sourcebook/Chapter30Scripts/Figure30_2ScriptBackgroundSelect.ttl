  x3do:sourceCode """
ecmascript:

function initialize () {
  Browser.println (' '); // skip line
  Browser.println ('Click shapes to select a background...');
  Browser.println (' '); // skip line
}

// setting the value of an eventOut variable also sends it as an event

function set_boolean ( value, eventTime ) {
  // only trigger on true values so that Background stays bound
  if ( value == true ) {  
	activated = value;
	Browser.println ('Cyan background ' + count);
  }
  else count++;  // received isActive value = false
}
""" .
:field_2_7_2_1 a owl:NamedIndividual, x3do:field ;
  x3do:hasParent :Filter1 ;
  x3do:accessType 'inputOnly' ;
  x3do:appinfo 'filter input value' ;
  x3do:name 'set_boolean' ;
  x3do:type 'SFBool' .
:field_2_7_2_2 a owl:NamedIndividual, x3do:field ;
  x3do:hasParent :Filter1 ;
  x3do:accessType 'outputOnly' ;
  x3do:appinfo 'activated provides a persistent binding value otherwise isActive events will toggle Background binding on/off.' ;
  x3do:name 'activated' ;
  x3do:type 'SFBool' .
:field_2_7_2_3 a owl:NamedIndividual, x3do:field ;
  x3do:hasParent :Filter1 ;
  x3do:accessType 'initializeOnly' ;
  x3do:appinfo 'count is only used locally so it is declared as an interface for persistence' ;
  x3do:name 'count' ;
  x3do:type 'SFInt32' ;
  x3do:value 1 .
:ROUTE_2_7_3 a owl:NamedIndividual, x3do:ROUTE ;
  x3do:hasParent :Group_2_7 ;
  x3do:fromField 'isActive' ;
  x3do:fromNode 'SensorButton1' ;
  x3do:toField 'set_boolean' ;
  x3do:toNode 'Filter1' .
