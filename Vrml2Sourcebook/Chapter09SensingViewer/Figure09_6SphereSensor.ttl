:Scene a owl:NamedIndividual, x3do:Scene ;
  x3do:hasParent :X3D ;
  x3do:hasChildren :WorldInfo_2_1, :FIRST_TRANSFORM, :SECOND_TRANSFORM ;
  x3do:hasROUTE :ROUTE_2_3, :ROUTE_2_5 .
:WorldInfo_2_1 a owl:NamedIndividual, x3do:WorldInfo ;
  x3do:hasParent :Scene ;
  x3do:title 'Figure09_6SphereSensor.x3d' .
:FIRST_TRANSFORM a owl:NamedIndividual, x3do:Transform ;
  x3do:hasParent :Scene ;
  x3do:hasChildren :Shape_2_2_1, :FIRST_SPHERESENSOR ;
  x3do:DEF 'FIRST_TRANSFORM' ;
  x3do:translation ( -2 0 0 ) .
:Shape_2_2_1 a owl:NamedIndividual, x3do:Shape ;
  x3do:hasParent :FIRST_TRANSFORM ;
  x3do:hasGeometry :Box_2_2_1_1 ;
  x3do:hasAppearance :FIRST_APPEARANCE .
:Box_2_2_1_1 a owl:NamedIndividual, x3do:Box ;
  x3do:hasParent :Shape_2_2_1 .
:FIRST_APPEARANCE a owl:NamedIndividual, x3do:Appearance ;
  x3do:hasParent :Shape_2_2_1 ;
  x3do:hasMaterial :Material_2_2_1_2_1 ;
  x3do:DEF 'FIRST_APPEARANCE' .
:Material_2_2_1_2_1 a owl:NamedIndividual, x3do:Material ;
  x3do:hasParent :FIRST_APPEARANCE ;
  x3do:diffuseColor ( .9 .5 .1 ) ;
  x3do:emissiveColor ( .2 .1 .05 ) .
:FIRST_SPHERESENSOR a owl:NamedIndividual, x3do:SphereSensor ;
  x3do:hasParent :FIRST_TRANSFORM ;
  x3do:DEF 'FIRST_SPHERESENSOR' ;
  x3do:description 'click and drag to rotate' .
:ROUTE_2_3 a owl:NamedIndividual, x3do:ROUTE ;
  x3do:hasParent :Scene ;
  x3do:fromField 'rotation_changed' ;
  x3do:fromNode 'FIRST_SPHERESENSOR' ;
  x3do:toField 'set_rotation' ;
  x3do:toNode 'FIRST_TRANSFORM' .
:SECOND_TRANSFORM a owl:NamedIndividual, x3do:Transform ;
  x3do:hasParent :Scene ;
  x3do:hasChildren :Shape_2_4_1, :SECOND_SPHERESENSOR ;
  x3do:DEF 'SECOND_TRANSFORM' ;
  x3do:translation ( 2 0 0 ) .
:Shape_2_4_1 a owl:NamedIndividual, x3do:Shape ;
  x3do:hasParent :SECOND_TRANSFORM ;
  x3do:hasGeometry :Cone_2_4_1_1 ;
  x3do:hasAppearance :FIRST_APPEARANCE-USE-1 .
:Cone_2_4_1_1 a owl:NamedIndividual, x3do:Cone ;
  x3do:hasParent :Shape_2_4_1 .
:FIRST_APPEARANCE-USE-1 a owl:NamedIndividual, x3do:Appearance ;
  x3do:hasParent :Shape_2_4_1 ;
  owl:sameAs :FIRST_APPEARANCE . # DEF matching this USE
:SECOND_SPHERESENSOR a owl:NamedIndividual, x3do:SphereSensor ;
  x3do:hasParent :SECOND_TRANSFORM ;
  x3do:DEF 'SECOND_SPHERESENSOR' ;
  x3do:description 'click and drag to rotate' .
:ROUTE_2_5 a owl:NamedIndividual, x3do:ROUTE ;
  x3do:hasParent :Scene ;
  x3do:fromField 'rotation_changed' ;
  x3do:fromNode 'SECOND_SPHERESENSOR' ;
  x3do:toField 'set_rotation' ;
  x3do:toNode 'SECOND_TRANSFORM' .
