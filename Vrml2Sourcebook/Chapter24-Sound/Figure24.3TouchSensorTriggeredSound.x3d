  <Scene>
    <Viewpoint description='click white ball for tone, markers indicate min/max range of sound' position='0 1 20'/>
    <Viewpoint description='20m distance, audible range circles at 5m and 10m' position='0 1 20'/>
    <NavigationInfo type='"WALK" "EXAMINE" "ANY"'/>
    <Group>
      <Sound minBack='5' minFront='5'>
        <AudioClip DEF='Source' description='tone1' url='"tone1.wav" "http://www.web3d.org/x3d/content/examples/Vrml2Sourcebook/Chapter24-Sound/tone1.wav"'/>
      </Sound>
      <Inline url='"Figure24.1EmitterMarkers.x3d" "http://www.web3d.org/x3d/content/examples/Vrml2Sourcebook/Chapter24-Sound/Figure24.1EmitterMarkers.x3d" "Figure24.1EmitterMarkers.wrl" "http://www.web3d.org/x3d/content/examples/Vrml2Sourcebook/Chapter24-Sound/Figure24.1EmitterMarkers.wrl"'/>
      <TouchSensor DEF='Touch' description='touch to start'/>
    </Group>
    <ROUTE fromField='touchTime' fromNode='Touch' toField='set_startTime' toNode='Source'/>
  </Scene>
</X3D>