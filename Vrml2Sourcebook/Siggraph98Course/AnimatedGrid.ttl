:Switcher a owl:NamedIndividual, x3do:Script ;
  x3do:hasParent :Scene ;
  x3do:hasField :field_2_8_1, :field_2_8_2, :field_2_8_3 ;
  x3do:DEF 'Switcher' ;
  x3do:sourceCode """
ecmascript:
    function set_fraction( frac, stamp ) {
            choice_changed = frac * numberOfChoices;
    }
""" .
:field_2_8_1 a owl:NamedIndividual, x3do:field ;
  x3do:hasParent :Switcher ;
  x3do:accessType 'outputOnly' ;
  x3do:name 'choice_changed' ;
  x3do:type 'SFInt32' .
:field_2_8_2 a owl:NamedIndividual, x3do:field ;
  x3do:hasParent :Switcher ;
  x3do:accessType 'initializeOnly' ;
  x3do:name 'numberOfChoices' ;
  x3do:type 'SFInt32' ;
  x3do:value 10 .
:field_2_8_3 a owl:NamedIndividual, x3do:field ;
  x3do:hasParent :Switcher ;
  x3do:accessType 'inputOnly' ;
  x3do:name 'set_fraction' ;
  x3do:type 'SFFloat' .
