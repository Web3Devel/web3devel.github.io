  x3do:hasParent :Scene ;
  x3do:hasChildren :Shape_2_5_1 ;
  x3do:translation ( -1.5 1.25 0.0 ) .
:Shape_2_5_1 a owl:NamedIndividual, x3do:Shape ;
  x3do:hasParent :Transform_2_5 ;
  x3do:hasGeometry :Message .
:Message a owl:NamedIndividual, x3do:Text ;
  x3do:hasParent :Shape_2_5_1 ;
  x3do:hasFontStyle :FontStyle_2_5_1_1_1 ;
  x3do:DEF 'Message' ;
  x3do:string '""' .
:FontStyle_2_5_1_1_1 a owl:NamedIndividual, x3do:FontStyle ;
  x3do:hasParent :Message ;
  x3do:family '"TYPEWRITER"' ;
  x3do:size 0.65 ;
  x3do:cssStyle 'BOLD' .
:Timer a owl:NamedIndividual, x3do:TimeSensor ;
  x3do:hasParent :Scene ;
  x3do:DEF 'Timer' ;
  x3do:cycleInterval '4.0' ;
  x3do:loop true .
:Introspect a owl:NamedIndividual, x3do:Script ;
  x3do:hasParent :Scene ;
  x3do:hasField :field_2_7_1, :field_2_7_2 ;
  x3do:DEF 'Introspect' ;
  x3do:sourceCode """
ecmascript:
		function update( ) {
			message.length = 5;
			message[0] = 'Browser: ' + Browser.getName( );
			message[1] = 'Version: ' + Browser.getVersion( );
			message[2] = 'URL:     ' + Browser.getWorldURL( );
			message[3] = 'Speed:   ' + Browser.getCurrentSpeed( );
			message[4] = 'Frames:  ' + Browser.getCurrentFrameRate( );
		}
		function initialize( ) {
			update( );
		}
		function trigger( t, ts ) {
			update( );
		}
""" .
:field_2_7_1 a owl:NamedIndividual, x3do:field ;
  x3do:hasParent :Introspect ;
  x3do:accessType 'inputOnly' ;
  x3do:name 'trigger' ;
  x3do:type 'SFTime' .
:field_2_7_2 a owl:NamedIndividual, x3do:field ;
  x3do:hasParent :Introspect ;
  x3do:accessType 'outputOnly' ;
  x3do:name 'message' ;
  x3do:type 'MFString' .
:ROUTE_2_8 a owl:NamedIndividual, x3do:ROUTE ;
  x3do:hasParent :Scene ;
  x3do:fromField 'cycleTime' ;
  x3do:fromNode 'Timer' ;
  x3do:toField 'trigger' ;
  x3do:toNode 'Introspect' .
:ROUTE_2_9 a owl:NamedIndividual, x3do:ROUTE ;
  x3do:hasParent :Scene ;
  x3do:fromField 'message' ;
  x3do:fromNode 'Introspect' ;
  x3do:toField 'set_string' ;
  x3do:toNode 'Message' .
