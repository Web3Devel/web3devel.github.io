NavigationInfo {
}
# Animation clock
DEF CLOCK TimeSensor {
  cycleInterval 4
  loop TRUE
}
# Key locations in the animation path are captured in the PositionInterpolator.
# Intermediate position values are interpolated between key locations.
DEF CUBEPATH PositionInterpolator {
  key [ 0.00 0.11 0.17 0.22 0.33 0.44 0.50 0.55 0.66 0.77 0.83 0.88 0.99 ]
  keyValue [ 0.0 0.0 0.0 1.0 1.96 1.0 1.5 2.21 1.5 2.0 1.96 2.0 3.0 0.0 3.0 2.0 1.96 3.0 1.5 2.21 3.0 1.0 1.96 3.0 0.0 0.0 3.0 0.0 1.96 2.0 0.0 2.21 1.5 0.0 1.96 1.0 0.0 0.0 0.0 ]
}
ROUTE CLOCK.fraction_changed TO CUBEPATH.set_fraction
# Moving box is rotated slightly to better show its shape.
DEF BOUNCINGBOXTRANSFORM Transform {
  rotation 0 1 0 .78
  children [
      Shape {
        appearance Appearance {
          material Material {
            diffuseColor .8 .8 .2
          }
        }
        geometry Box {
          size 1 1.5 1
        }
      }
  ]
}
ROUTE CUBEPATH.value_changed TO BOUNCINGBOXTRANSFORM.set_translation
