:Scene a owl:NamedIndividual, x3do:Scene ;
  x3do:hasParent :X3D ;
  x3do:hasChildren :WorldInfo_2_1, :Viewpoint_2_2, :NavigationInfo_2_3, :Transform_2_4, :Transform_2_5, :Timer, :Introspect ;
  x3do:hasROUTE :ROUTE_2_8, :ROUTE_2_9 .
:WorldInfo_2_1 a owl:NamedIndividual, x3do:WorldInfo ;
  x3do:hasParent :Scene ;
  x3do:info '"Copyright (c) 1998, David R. Nadeau"' ;
  x3do:title 'Query browser state' .
:Viewpoint_2_2 a owl:NamedIndividual, x3do:Viewpoint ;
  x3do:hasParent :Scene ;
  x3do:description 'Entry view' .
:NavigationInfo_2_3 a owl:NamedIndividual, x3do:NavigationInfo ;
  x3do:hasParent :Scene .
:Transform_2_4 a owl:NamedIndividual, x3do:Transform ;
  x3do:hasParent :Scene ;
  x3do:hasChildren :Inline_2_4_1 ;
  x3do:scale ( 0.75 0.75 0.75 ) ;
  x3do:translation ( -4.0 0.0 0.0 ) .
:Inline_2_4_1 a owl:NamedIndividual, x3do:Inline ;
  x3do:hasParent :Transform_2_4 ;
  x3do:url '"Two.x3d" "https://www.web3d.org/x3d/content/examples/Vrml2Sourcebook/Siggraph98Course/Two.x3d" "Two.wrl" "https://www.web3d.org/x3d/content/examples/Vrml2Sourcebook/Siggraph98Course/Two.wrl"' .
