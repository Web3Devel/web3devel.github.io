  x3do:hasParent :Appearance_2_3_3_1_1 ;
  x3do:DEF 'BULB_MATERIAL' ;
  x3do:diffuseColor ( 0.4 0.4 0.4 ) ;
  x3do:emissiveColor ( 0.8 0.8 0.8 ) .
