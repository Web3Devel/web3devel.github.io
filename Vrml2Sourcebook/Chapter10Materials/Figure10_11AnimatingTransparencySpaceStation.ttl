:Scene a owl:NamedIndividual, x3do:Scene ;
  x3do:hasParent :X3D ;
  x3do:hasChildren :WorldInfo_2_1, :Viewpoint_2_2, :Group_2_3 ;
  x3do:hasROUTE :ROUTE_2_4, :ROUTE_2_5 .
:WorldInfo_2_1 a owl:NamedIndividual, x3do:WorldInfo ;
  x3do:hasParent :Scene ;
  x3do:title 'Figure10_11AnimatingTransparencySpaceStation.x3d' .
:Viewpoint_2_2 a owl:NamedIndividual, x3do:Viewpoint ;
  x3do:hasParent :Scene ;
  x3do:description '5m space station from 8m away' ;
  x3do:position ( 0 0 8 ) .
:Group_2_3 a owl:NamedIndividual, x3do:Group ;
  x3do:hasParent :Scene ;
  x3do:hasChildren :Shape_2_3_1, :Shape_2_3_2, :Shape_2_3_3, :CLOCK, :TRANSPARENCY_PATH .
:Shape_2_3_1 a owl:NamedIndividual, x3do:Shape ;
  x3do:hasParent :Group_2_3 ;
  x3do:hasAppearance :Appearance_2_3_1_1 ;
  x3do:hasGeometry :Sphere_2_3_1_2 .
:Appearance_2_3_1_1 a owl:NamedIndividual, x3do:Appearance ;
  x3do:hasParent :Shape_2_3_1 ;
  x3do:hasMaterial :BALL_MATERIAL .
:BALL_MATERIAL a owl:NamedIndividual, x3do:Material ;
  x3do:hasParent :Appearance_2_3_1_1 ;
  x3do:DEF 'BALL_MATERIAL' ;
  x3do:diffuseColor ( 1 0 0 ) .
:Sphere_2_3_1_2 a owl:NamedIndividual, x3do:Sphere ;
  x3do:hasParent :Shape_2_3_1 .
:Shape_2_3_2 a owl:NamedIndividual, x3do:Shape ;
  x3do:hasParent :Group_2_3 ;
  x3do:hasAppearance :Appearance_2_3_2_1 ;
  x3do:hasGeometry :Cylinder_2_3_2_2 .
:Appearance_2_3_2_1 a owl:NamedIndividual, x3do:Appearance ;
  x3do:hasParent :Shape_2_3_2 ;
  x3do:hasMaterial :Material_2_3_2_1_1 .
:Material_2_3_2_1_1 a owl:NamedIndividual, x3do:Material ;
  x3do:hasParent :Appearance_2_3_2_1 ;
  x3do:diffuseColor ( 0.5 0.25 1 ) .
:Cylinder_2_3_2_2 a owl:NamedIndividual, x3do:Cylinder ;
  x3do:hasParent :Shape_2_3_2 ;
  x3do:height 0.05 ;
  x3do:radius 2 .
:Shape_2_3_3 a owl:NamedIndividual, x3do:Shape ;
  x3do:hasParent :Group_2_3 ;
  x3do:hasAppearance :Appearance_2_3_3_1 ;
  x3do:hasGeometry :Cylinder_2_3_3_2 .
:Appearance_2_3_3_1 a owl:NamedIndividual, x3do:Appearance ;
  x3do:hasParent :Shape_2_3_3 ;
  x3do:hasMaterial :Material_2_3_3_1_1 .
:Material_2_3_3_1_1 a owl:NamedIndividual, x3do:Material ;
  x3do:hasParent :Appearance_2_3_3_1 ;
  x3do:diffuseColor ( 0.75 0 1 ) .
:Cylinder_2_3_3_2 a owl:NamedIndividual, x3do:Cylinder ;
  x3do:hasParent :Shape_2_3_3 ;
  x3do:height 5 ;
  x3do:radius 0.15 .
:CLOCK a owl:NamedIndividual, x3do:TimeSensor ;
  x3do:hasParent :Group_2_3 ;
  x3do:DEF 'CLOCK' ;
  x3do:cycleInterval '10' ;
  x3do:loop true .
:TRANSPARENCY_PATH a owl:NamedIndividual, x3do:ScalarInterpolator ;
  x3do:hasParent :Group_2_3 ;
  x3do:DEF 'TRANSPARENCY_PATH' ;
  x3do:key ( 0 0.45 0.9 1 ) ;
  x3do:keyValue ( 0 1 0 0 ) .
:ROUTE_2_4 a owl:NamedIndividual, x3do:ROUTE ;
  x3do:hasParent :Scene ;
  x3do:fromField 'fraction_changed' ;
  x3do:fromNode 'CLOCK' ;
  x3do:toField 'set_fraction' ;
  x3do:toNode 'TRANSPARENCY_PATH' .
:ROUTE_2_5 a owl:NamedIndividual, x3do:ROUTE ;
  x3do:hasParent :Scene ;
  x3do:fromField 'value_changed' ;
  x3do:fromNode 'TRANSPARENCY_PATH' ;
  x3do:toField 'set_transparency' ;
  x3do:toNode 'BALL_MATERIAL' .
